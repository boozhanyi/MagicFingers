{
  "expo": {
    "name": "MagicFingers",
    "slug": "MagicFingers",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/MagicFingers_Logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Loading_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#C5FFFF",
      "plugins": [
        [
          "expo-image-picker",
          {
            "photosPermission": "The app accesses your photos to let you share them with your friends."
          },
          "expo-media-library",
          {
            "photosPermission": "Allow $(PRODUCT_NAME) to access your photos.",
            "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos.",
            "isAccessMediaLocationEnabled": true
          }
        ]
      ]
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundImage": "./assets/MagicFingers_Logo.png"
      },
      "package": "com.boozhanyi.MagicFingers"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "b9503f12-b9e4-4475-87ab-f677764ab3da"
      }
    }
  }
}
